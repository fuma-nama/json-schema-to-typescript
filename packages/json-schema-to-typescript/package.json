{
  "name": "@fumari/json-schema-to-typescript",
  "version": "1.1.3",
  "description": "compile json schema to typescript typings",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts",
    "dev": "tsup --watch"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuma-nama/json-schema-to-typescript.git"
  },
  "keywords": [
    "json",
    "schema",
    "typescript",
    "compile",
    "transpile",
    "api",
    "interface",
    "typing",
    "share"
  ],
  "author": "Fuma Nama; Boris Cherny",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.9.3",
    "js-yaml": "^4.1.0",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
