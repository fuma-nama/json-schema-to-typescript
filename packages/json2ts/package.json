{
  "name": "@fumari/json2ts",
  "version": "2.0.0",
  "description": "a CLI tool to compile json schema to typescript typings",
  "bin": {
    "json2ts": "./dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts",
    "dev": "tsup --watch"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuma-nama/json-schema-to-typescript.git"
  },
  "keywords": [
    "json",
    "schema",
    "typescript",
    "compile",
    "transpile",
    "api",
    "interface",
    "typing",
    "share",
    "cli"
  ],
  "author": "Fuma Nama; Boris Cherny",
  "license": "MIT",
  "dependencies": {
    "@fumari/json-schema-to-typescript": "workspace:*",
    "glob": "^13.0.6",
    "is-glob": "^4.0.3",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@types/is-glob": "^4.0.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^25.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
