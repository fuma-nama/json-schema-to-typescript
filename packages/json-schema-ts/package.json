{
  "name": "@fumari/json-schema-ts",
  "version": "0.0.2",
  "description": "compile json schema to typescript typings",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "eslint src/**/*.ts",
    "dev": "tsdown --watch"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuma-nama/json-schema-to-typescript.git"
  },
  "keywords": [
    "json",
    "schema",
    "typescript",
    "compile",
    "transpile",
    "api",
    "interface",
    "typing",
    "share"
  ],
  "author": "Fuma Nama",
  "license": "MIT",
  "dependencies": {
    "esrap": "^2.2.3"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^15.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.3.2",
    "@typescript-eslint/types": "^8.56.1",
    "js-yaml": "^4.1.1",
    "json-schema-typed": "^8.0.2",
    "tsdown": "0.21.0-beta.2"
  },
  "peerDependencies": {
    "json-schema-typed": "^8.0.2"
  },
  "peerDependenciesMeta": {
    "json-schema-typed": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  }
}
